- skills_list.each do |object_from_list|
  %span.help_cursor{"data-target" => "#skill_modal_#{object_from_list.skill.id}", "data-toggle" => "modal", :type => "button"}
    / write_school_skills is defined in legendopedia_helper
    = write_school_skills(object_from_list, skills_list)
  = render partial: 'shared/pop_up_window', locals: { head_object: object_from_list.skill.name_pl, desc_object: object_from_list.skill.desc, id_name: "skill_modal_#{object_from_list.skill.id}" } 